            "polyfills": "projects/shop/src/polyfills.ts",
            "tsConfig": "projects/shop/tsconfig.spec.json",
            "karmaConfig": "projects/shop/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/shop/src/favicon.ico",
              "projects/shop/src/assets"
            ],
            "styles": ["projects/shop/src/styles.scss"],
            "scripts": []
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/shop/server",
            "main": "projects/shop/server.ts",
            "tsConfig": "projects/shop/tsconfig.server.json",
            "inlineStyleLanguage": "scss"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "projects/shop/src/environments/environment.ts",
                  "with": "projects/shop/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "shop:build:development",
              "serverTarget": "shop:server:development"
            },
            "production": {
              "browserTarget": "shop:build:production",
              "serverTarget": "shop:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": ["/"]
          },
          "configurations": {
            "production": {
              "browserTarget": "shop:build:production",
              "serverTarget": "shop:server:production"
            },
            "development": {
              "browserTarget": "shop:build:development",
              "serverTarget": "shop:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
